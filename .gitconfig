[alias]
        ls = log --graph --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
        visual = "!gitk &"
        prune-all = "!git remote | xargs -n 1 git remote prune"
        emergency = "!f() { \
                       sh ~/gitScripts/gitEmergency.sh $1; \
                     }; f"
